<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:src="@drawable/mohamad"

        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="자바"
            android:textSize="20dp"/>

        <RatingBar android:id="@+id/mhmd_java_Average"
            android:paddingTop="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="5"
            android:rating="0"
            style="?android:attr/ratingBarStyleSmall"/>

        <Button
            android:id="@+id/mhmd_java"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="평판"
            android:onClick="onClick"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="자료구조"
            android:textSize="20dp"/>
        <RatingBar android:id="@+id/mhmd_datastructure_Average"
            android:paddingTop="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="5"
            android:rating="0"
            style="?android:attr/ratingBarStyleSmall"/>
        <Button
            android:id="@+id/mhmd_datastructure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="평판"
            android:onClick="onClick"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="알고리즘"
            android:textSize="20dp"/>
        <RatingBar android:id="@+id/mhmd_algo_Average"
            android:paddingTop="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="5"
            android:rating="0"
            style="?android:attr/ratingBarStyleSmall"/>
        <Button
            android:id="@+id/mhmd_algo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="평판"
            android:textSize="20dp"
            android:onClick="onClick"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="데이터베이스프로그래밍"
            android:textSize="20dp"/>
        <RatingBar android:id="@+id/mhmd_databasepro_Average"
            android:paddingTop="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="5"
            android:rating="0"
            style="?android:attr/ratingBarStyleSmall"/>
        <Button
            android:id="@+id/mhmd_databasepro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="평판"
            android:textSize="20dp"
            android:onClick="onClick"/>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/mhmdcall"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="centerCrop"
            android:onClick="onClickCall"
            android:src="@drawable/ic_contact_phone_black_24dp"/>

        <ImageButton
            android:id="@+id/mhmdmail"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="centerCrop"
            android:onClick="onClickMail"
            android:src="@drawable/ic_contact_mail_black_24dp"/>


    </LinearLayout>




</LinearLayout>